              .tel-num
                span.tel-num-pref +7 (917)
                span.tel-num-num 557-57-55
              .tel-descr Круглосуточно
        .slide-in
          h1 Аренда автомобилей в Сочи
          h3.h3-info обслуживание корпоративных мероприятий
          h3.h3-type mercedes «e», «S», «V»  class,  viano,  sprinter
      .bid
        .bid-in
          h1 онлайн-заявка
          .tab.js-tab-group
            ul.tab-nav.js-tab
              li.m-first
                a.tab-link(href="js-tab1") Мероприятие
              li
                a.tab-link(href="js-tab2") Свадебный экскорт
              li.m-last
                a.tab-link(href="js-tab3") Индивидуальный трансфер
            .tab-content
              .tab-item.js-tab-cont.js-tab1
                .form
                 .form-row 
                  .form-col.m-col1
                    .form-title Дата мероприятия
                    .form-date
                      input(type="text" placeholder="21 марта 2014").form-date-day.js-datepicker
                      i.form-date-ico
                  .form-col.m-col2
                    .form-title Выбор автомобилей
                    .form-select.js-select
                      span.form-select-meta Выбрать из списка...
                      i.form-select-ico
                    .form-list
                      ul
                        li
                          .form-list-item 
                            .form-list-auto
                              span Mercedes S class 
                              span.form-list-note до 3 чел. 
                            .form-list-summ кол-во авто: 4
                            i.form-list-del
                        li
                          .form-list-item 
                            .form-list-auto
                              span Mercedes Viano 
                              span.form-list-note до 8 чел.
                            .form-list-summ кол-во авто: 1
                            i.form-list-del
                  .form-col.m-col3
                    .form-title Ваш телефон или e-mail
                    input(type="text" placeholder="+7 906 566-33-00").input
                  .form-col.m-col4
                    .form-title
                    button.form-btn ОТПРАВИТЬ ЗАЯВКУ
